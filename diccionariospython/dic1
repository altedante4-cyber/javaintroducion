#liga.clear() #elimina el diccionario compeltamente 
print(liga) 

#el metodo pop localisa un elemento primero y luego lo elimina 
valor = liga.pop("equipo2")
print(valor)
print(liga)

#ponemos una clave que no existe nos da una excepcion 

valor = liga.pop("equipo22","ese equipo no existe") #esto devuelve la lista por que le pasamos un segundo parametro peroo si solo le pasos un parameto es decir euipo22  nos provoca una excepsion que no queremos que nos provoque 

#devuelve el ultimo elemento introducido

valor = liga.popitem()  #borra el ultimoelemento
valor = liga.popitem() 
print(liga) 

supercopa = {'equipo6':11 , 'equipo1':55,'equipo4':33}
nuevoDiccionario = liga.copy()
nuevoDiccionario.update(supercopa)
print(liga)
print(supercopa)
print(nuevoDiccionario) 


